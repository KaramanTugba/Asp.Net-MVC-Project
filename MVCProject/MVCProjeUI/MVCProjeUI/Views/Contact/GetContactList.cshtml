@using EntityLayer.Concrete;
@model List<Contact>
@{
    ViewBag.Title = "GetContactList";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

<h2>Contact List</h2>
<table class="table table-hover">
    <tr>
        <td>Contact ID</td>
        <td>User Name</td>
        <td>User Mail</td>
        <td>Subject</td>
        <td>Message</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ContactId</td>
            <td>@item.UserName</td>
            <td>@item.UserMail</td>
            <td>@item.Subject</td>
            <td>@item.Message</td>
        </tr>
    }
</table>
<a href="/Contact/AddContact" class="btn btn-info">New Contact</a>